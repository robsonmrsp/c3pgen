{
  "env": {
    "development": {
      "presets": [ "es2015", "stage-0" ],
      "plugins": [
        ["transform-es2015-classes", { "loose": true }],
        "transform-proto-to-assign"
      ]
    },
    "rollup": {
      "presets": [ "es2015-rollup", "stage-0"  ],
      "plugins": [
        ["transform-es2015-classes", { "loose": true }],
        "transform-proto-to-assign"
      ]
    },
    "jsnext": {
      "presets": [ "es2015-native-modules", "stage-0"  ],
      "plugins": [
        ["transform-es2015-classes", { "loose": true }],
        "transform-proto-to-assign"
      ]
    }
  }
}
